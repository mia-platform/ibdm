{
	"apiVersion": "group/v1",
	"itemFamily": "configs",
	"type": "json",
	"syncable": true,
	"mappings": {
		"identifier": "{{ .name }}",
		"spec": {
			"key": "{{ .value }}",
			"otherKey": "{{ .otherValue | functionName }}"
		},
		"extra": [
			{
				"apiVersion": "group/v1",
				"itemFamily": "relationships",
				"deletePolicy": "none",
				"identifier": "extra1",
				"sourceRef": {
					"extraKey": "{{ .extraValue }}"
				},
				"type": "dependency"
			}
		]
	}
}
